{
  "name": "frida-panic",
  "version": "1.1.1",
  "description": "Easy crash-reporting for Frida-based applications",
  "main": "index.js",
  "files": [
    "/index.js"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            [
              "@babel/plugin-transform-runtime",
              {
                "corejs": 2
              }
            ]
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nowsecure/frida-panic.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nowsecure/frida-panic/issues"
  },
  "homepage": "https://github.com/nowsecure/frida-panic#readme",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs2": "^7.1.2",
    "babelify": "^10.0.0"
  }
}
