{
  "name": "frida-panic",
  "version": "1.1.1",
  "description": "Easy crash-reporting for Frida-based applications",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "es2015",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            "transform-runtime"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nowsecure/frida-panic.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nowsecure/frida-panic/issues"
  },
  "homepage": "https://github.com/nowsecure/frida-panic#readme",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0"
  }
}
